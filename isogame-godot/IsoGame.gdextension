[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../isogame-rs/target/debug/libisogame_rs.so"
linux.release.x86_64 =   "res://../isogame-rs/target/release/libisogame_rs.so"
windows.debug.x86_64 =   "res://../isogame-rs/target/x86_64-pc-windows-gnu/debug/isogame_rs.dll"
windows.release.x86_64 = "res://../isogame-rs/target/x86_64-pc-windows-gnu/release/isogame_rs.dll"
macos.debug =            "res://../isogame-rs/target/debug/libisogame_rs.dylib"
macos.release =          "res://../isogame-rs/target/release/libisogame_rs.dylib"
macos.debug.arm64 =      "res://../isogame-rs/target/debug/libisogame_rs.dylib"
macos.release.arm64 =    "res://../isogame-rs/target/release/libisogame_rs.dylib"
